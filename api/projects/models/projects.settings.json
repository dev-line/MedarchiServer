{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Projects"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Published": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
